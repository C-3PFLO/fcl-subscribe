{
    "name": "fcl-subscribe",
    "version": "0.1.3",
    "description": "Subscribe to Flow blockchain events",
    "main": "build/fcl-subscribe.js",
    "scripts": {
        "build": "babel src demo -d build",
        "check": "./node_modules/.bin/eslint src test",
        "test": "jest --coverage",
        "test:watch": "jest --coverage --watch",
        "demo": "npm run build; node build/demo.js"
    },
    "jest": {
        "reporters": [
            "default",
            "jest-junit"
        ],
        "coverageDirectory": "artifacts/coverage",
        "coveragePathIgnorePatterns": [
            "test"
        ]
    },
    "jest-junit": {
        "outputDirectory": "artifacts"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/C-3PFLO/fcl-subscribe.git"
    },
    "author": "C-3PFLO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/C-3PFLO/fcl-subscribe/issues"
    },
    "homepage": "https://github.com/C-3PFLO/fcl-subscribe#readme",
    "devDependencies": {
        "eslint": "^8.6.0",
        "eslint-config-google": "^0.14.0",
        "eslint-plugin-jest": "^23.17.1",
        "jest": "^27.4.7",
        "jest-junit": "^13.0.0",
        "@onflow/fcl": "^0.0.78",
        "@onflow/types": "^0.0.6"
    },
    "dependencies": {
        "@babel/cli": "^7.10.3",
        "@babel/core": "^7.10.3",
        "@babel/preset-env": "^7.10.3"
    }
